        "key": "escape",
        "when": "leaderkeyState",
        "command": "runCommands",
        "args": {
          "commands": [
            {
              "command": "_setContext",
              "args": [
                "leaderkeyState",
                ""
              ]
            },
            {
              "command": "leaderkey.render",
              "args": ""
            }
          ]
        }
      },
      {
        "key": "backspace",
        "when": "leaderkeyState",
        "command": "leaderkey.onkey",
        "args": "<back>"
      }
    ]
